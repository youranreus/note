    "@nuxtjs/tailwindcss": "^6.11.4",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@reus-able/types": "^1.0.5",
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/core": "^10.7.2",
    "@vueuse/nuxt": "^10.7.2",
    "naive-ui": "^2.37.3",
    "prisma": "^5.9.1",
